		"graphql": "^15.5.1",
		"handlebars": "^4.7.7",
		"helmet": "^4.6.0",
		"reflect-metadata": "^0.1.13",
		"sqlite3": "^5.0.2",
		"type-graphql": "^1.1.1",
		"typedi": "^0.10.0",
		"typeorm": "^0.2.34",
		"typeorm-typedi-extensions": "^0.4.1"
	}
